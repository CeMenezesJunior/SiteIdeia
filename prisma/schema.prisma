// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "sqlite:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Post{
  id Int @id @default(autoincrement())
  title String
  except String
  createAt DateTime @default(now())
}

model Repositorio{
  id Int @id @default(autoincrement())
  titulo String
  linkGitHub String
  linkSite String?
  descricao String
  
}